class Solution {
public:
    int findDuplicate(vector<int>& nums) {
    int a = nums[nums[0]];
    int b = nums[nums[nums[0]]];
    while (a != b) {
      a = nums[a];
      b = nums[nums[b]];
    }
    a = nums[0];
    while (a != b) {
      a = nums[a];
      b = nums[b];
    }
    return a;   
    }
};
